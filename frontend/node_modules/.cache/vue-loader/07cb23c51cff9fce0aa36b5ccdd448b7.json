mponent: {\n    MovieSelect,\n  },\n  data() {\n    return {\n      movies: []\n    };\n  },\n  created() {\n      let url = 'http://127.0.0.1:8000/api/v1/movie/' // 장고의 서버주소\n      axios.get(url)\n      .then(function(response){\n        console.log(response);\n        this.movies = response.data\n      })\n      .catch(function(response){\n        console.log(response);\n      })\n    },\n}\n</script>\n\n<style>\n\n</style>"]}]}